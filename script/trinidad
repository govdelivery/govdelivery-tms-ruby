#!/usr/bin/env jruby

# IPAWS needs to be loaded before trinidad otherwise CXF will raise this dreaded error:
# com.sun.xml.ws.client.sei.SEIStub cannot be cast to org.apache.cxf.frontend.ClientProxy
require 'synapse'
require 'lock_jar'
LockJar.load

require 'trinidad'

Trinidad::CommandLineParser.parse(ARGV)
Trinidad::Server.new(Trinidad.configuration).start